package com.sportspredictor.service;

import java.util.Date;

public class EventoParams {
  public final String id;
  public final String nombre;
  public final Date fecha;

  public EventoParams(String id, String nombre, Date fecha) {
    this.id = id; this.nombre = nombre; this.fecha = new Date(fecha.getTime());
  }
}

